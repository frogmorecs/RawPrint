<Project Sdk="Microsoft.NET.Sdk" ToolsVersion="15.0">
  <PropertyGroup>
    <TargetFrameworks>net40;netstandard2.0</TargetFrameworks>
  </PropertyGroup>
  <PropertyGroup>
    <PackageId>RawPrint</PackageId>
    <AssemblyTitle>Raw Print</AssemblyTitle>
    <Description>.Net library to send files directly to a Windows printer bypassing the printer driver.</Description>
    <PackageLicenseExpression>BSD-2-Clause-FreeBSD</PackageLicenseExpression>
    <PackageRequireLicenseAcceptance>False</PackageRequireLicenseAcceptance>
    <PackageProjectUrl>https://github.com/tonyedgecombe/RawPrint</PackageProjectUrl>
    <PackageTags>RawPrint Raw Print PCL PostScript</PackageTags>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
  </PropertyGroup>
</Project>